{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    /*"module": "esnext",*/
    "module": "commonjs",
    
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/UI/organism/popUp.jsx", "src/components/UI/organism/popUp.jsx", "src/components/UI/atom/button.tsx", "lib/validation.js", "src/components/UI/molecule/registerForm.jsx", "src/components/UI/atom/inputForm.jsx"],
  "exclude": ["node_modules"]
}
